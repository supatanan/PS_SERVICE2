  object main_select: TUniQuery
    Connection = MyMD.MyCon
    SQL.Strings = (
      'SELECT'
      ''
      'tbmember.username,'
      'tbmember.`password`,'
      'tbmember.firstname,'
      'tbmember.lastname,'
      'tbmember.Vdate,'
      'tbmember.email,'
      'tbmember.dept_id,'
      'tbmember.post_id,'
      'tbmember.ac_id,'
      'tbposition.post_id,'
      'tbposition.post_name,'
      'tbdepartment.dept_id,'
      'tbdepartment.dept_name,'
      'tbactive_status.ac_id,'
      'tbactive_status.ac_name'
      ''
      'FROM'
      'tbmember'
      'INNER JOIN tbposition ON tbposition.post_id = tbmember.post_id'
      
        'INNER JOIN tbdepartment ON tbdepartment.dept_id = tbmember.dept_' +
        'id'
      
        'INNER JOIN tbactive_status ON tbactive_status.ac_id = tbmember.a' +
        'c_id'
      'ORDER BY tbmember.Vdate DESC')
    BeforePost = main_selectBeforePost
